    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "10 loops, best of 3: 94.2 ms per loop\n"
     ]
    }
   ],
   "source": [
    "%timeit df.groupby('ticker').apply(np.mean)"
   ]
  },
  {
   "cell_type": "code",
